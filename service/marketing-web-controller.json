{
   "kind":"ReplicationController",
   "apiVersion":"v1",
   "metadata":{
      "name":"marketing-web",
      "labels":{
         "name":"marketing-web"
      }
   },
   "spec":{
      "replicas":2,
      "selector":{
         "name":"marketing-web"
      },
      "template":{
         "metadata":{
            "labels":{
               "name":"marketing-web"
            }
         },
         "spec":{
            "containers":[
               {
                  "name":"marketing-web",
                  "image":"sundbry/etheride-marketing",
                  "ports":[
                     {
                        "containerPort":3744
                     }
                  ]
               }
            ]
         }
      }
   }
}
