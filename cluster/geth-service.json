{
   "kind":"Service",
   "apiVersion":"v1",
   "metadata":{
      "name":"geth-node",
      "labels":{
         "name":"geth-node"
      }
   },
   "spec":{
      "ports": [
        {
          "name":"eth-rpc",
          "port":8545,
          "targetPort":8545
        },
        {
          "name":"eth-p2p",
          "port": 30303,
          "targetPort": 30303
        }
      ],
      "selector":{
         "role":"geth-node"
      },
      "type":"NodePort"
   }
}
